{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/v1/Security/auth/login",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "security.api",
          "Port": "80"
        }
      ],
      "UpstreamPathTemplate": "/auth/login",
      "UpstreamHttpMethod": [ "POST" ]
    },
    {
      "DownstreamPathTemplate": "/api/v1/Security/users",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "security.api",
          "Port": "80"
        }
      ],
      "UpstreamPathTemplate": "/users",
      "UpstreamHttpMethod": [ "GET", "POST" ]
    },
    {
      "DownstreamPathTemplate": "/api/v1/Security/users/{userId}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "security.api",
          "Port": "80"
        }
      ],
      "UpstreamPathTemplate": "/users/{userId}",
      "UpstreamHttpMethod": [ "GET", "PUT", "DELETE" ]
    },
    {
      "DownstreamPathTemplate": "/api/v1/Security/users/{userId}/roles",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "security.api",
          "Port": "80"
        }
      ],
      "UpstreamPathTemplate": "/users/{userId}/roles",
      "UpstreamHttpMethod": [ "GET", "POST" ]
    },
    {
      "DownstreamPathTemplate": "/api/v1/Security/users/{userId}/roles/{roleId}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "security.api",
          "Port": "80"
        }
      ],
      "UpstreamPathTemplate": "/users/{userId}/roles",
      "UpstreamHttpMethod": [ "DELETE" ]
    },
    {
      "DownstreamPathTemplate": "/api/v1/Security/roles/{roleId}/pantallas",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "security.api",
          "Port": "80"
        }
      ],
      "UpstreamPathTemplate": "/roles/{roleId}/pantallas",
      "UpstreamHttpMethod": [ "POST" ]
    }

  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5010"
  }
}
